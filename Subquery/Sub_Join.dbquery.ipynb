   "source": [
    "### Find all the customers and find total orders for each customers \n",
    "\n",
    "select c.customerid,c.CustomerName,c.City, o.cnt from customers c\n",
    "left join (select CustomerID, count(*) as cnt from orders group by CustomerID order by cnt desc) as o\n",
    "on c.CustomerID = o.CustomerID"
   ]
