   "source": [
    "### Find all the customers and find total orders for each customers \n",
    "\n",
    "SELECT \n",
    "  c.*, \n",
    "  o.cnt \n",
    "FROM customers c\n",
    "LEFT JOIN (\n",
    "  SELECT \n",
    "    CustomerID, \n",
    "    COUNT(*) AS cnt \n",
    "  FROM orders \n",
    "  GROUP BY CustomerID\n",
    "  ORDER BY cnt DESC\n",
    ") AS o\n",
    "ON c.CustomerID = o.CustomerID"
   ]
  }
 ],
