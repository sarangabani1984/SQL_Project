    "select * from workspace.sarang.bookstore_data\n",
    "\n",
    "SELECT\n",
    "  Book_ID,\n",
    "  Price\n",
    "FROM (\n",
    "  SELECT \n",
    "    Book_ID,\n",
    "    Price, \n",
    "    AVG(Price) OVER () AS avg_price \n",
    "  FROM workspace.sarang.bookstore_data\n",
    ") t\n",
    "WHERE Price > t.avg_price\n",
    "\n"
