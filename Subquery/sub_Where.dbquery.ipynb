   "source": [
    "--  \"Find the products that have a price higher than the average price of all products.\"\n",
    "\n",
    "SELECT *,(select avg(price) FROM products) avgprice\n",
    "FROM products\n",
    "WHERE price > (SELECT AVG(price) FROM products)\n",
    "ORDER BY price DESC;"
